<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:width="60dp" android:height="60dp">
        <shape android:shape="oval" xmlns:android="http://schemas.android.com/apk/res/android">
           <gradient
               android:startColor="@color/undifned_color"
               android:endColor="#01888888"
                android:gradientRadius="44dp"
               android:centerX="0.5"
               android:centerY="0.5"
               android:type="radial"/>
        </shape>
    </item>
    <item android:width="48dp" android:height="48dp" android:left="6dp" android:top="6dp">
        <shape android:shape="oval" xmlns:android="http://schemas.android.com/apk/res/android">
            <solid android:color="@color/undifned_color"/>
        </shape>
    </item>
</layer-list>