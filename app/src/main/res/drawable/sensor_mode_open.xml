<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:width="60dp" android:height="60dp">
        <shape android:shape="oval" xmlns:android="http://schemas.android.com/apk/res/android">
           <gradient
               android:startColor="@color/open_color"
               android:endColor="#01057ac5"
                android:gradientRadius="30dp"
               android:centerX="0.5"
               android:centerY="0.5"
               android:type="radial"/>
        </shape>
    </item>
    <item android:width="32dp" android:height="32dp" android:left="14dp" android:top="14dp">
        <shape android:shape="oval" xmlns:android="http://schemas.android.com/apk/res/android">
            <solid android:color="@color/open_color"/>
        </shape>
    </item>
</layer-list>